{
  "name": "expresso-mongoose",
  "version": "1.1.0",
  "description": "express, mongodb & typescript",
  "main": "src/server.ts",
  "private": true,
  "scripts": {
    "key:generate": "npx ts-node ./src/@expresso/scripts/generate.ts",
    "dev": "npm-run-all build start:dev",
    "start": "nodemon --exec 'ts-node' ./src/server.ts",
    "start:dev": "nodemon --exec node ./dist/server.js",
    "clean": "rm -rf ./dist",
    "test:lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "test:types": "tsc --noEmit",
    "test:types-watch": "npm run test:types -- --watch",
    "prebuild": "yarn && npm-run-all clean test:lint test:types",
    "build": "npx tsc -p .",
    "deploy": "npm run build && pm2 reload ecosystem.config.js",
    "serve:staging": "NODE_ENV=staging node ./dist/server.js",
    "serve:production": "NODE_ENV=production node ./dist/server.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/server.js",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "mongoose",
    "typescript"
  ],
  "author": "masb0ymas",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.53.1",
    "@aws-sdk/s3-request-presigner": "^3.161.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "convert-excel-to-json": "^1.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-useragent": "^1.0.15",
    "googleapis": "^95.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "http-errors": "^2.0.0",
    "i18next": "^21.6.13",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.0",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.9.49",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "5.13.14",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.2",
    "nodemailer-mailgun-transport": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "randomstring": "^1.2.2",
    "rate-limiter-flexible": "^2.3.6",
    "redis": "^3.1.2",
    "request-ip": "^2.1.3",
    "sharp": "^0.30.2",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "yup": "0.31.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/convert-excel-to-json": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/ioredis": "^4.28.8",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.179",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/node-cron": "^3.0.1",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/randomstring": "^1.1.8",
    "@types/redis": "^2.8.32",
    "@types/request-ip": "^0.0.37",
    "@types/sharp": "^0.29.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "_moduleAliases": {
    "@expresso/*": "dist/@expresso/*",
    "@config/*": "dist/config/*",
    "@controllers/*": "dist/controllers/*",
    "@database/*": "dist/database/*",
    "@jobs/*": "dist/jobs/*",
    "@middlewares/*": "dist/middlewares/*",
    "@routes/*": "dist/routes/*",
    "@utils/*": "dist/utils/*",
    "@views/*": "dist/views/*"
  }
}
